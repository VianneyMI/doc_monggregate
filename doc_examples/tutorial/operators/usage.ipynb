   ],
   "source": [
    "pipeline = Pipeline()\n",
    "\n",
    "pipeline.group(\n",
    "    by=\"year\",\n",
    "    query={\n",
    "        \"movie_count\": S.sum(1),\n",
    "        \"movie_titles\": S.push(S.field(\"title\"))\n",
    "    }\n",
    ")"
   ]
